{
  "name": "myxen-token-tools",
  "version": "0.1.0",
  "private": true,
  "description": "Scripts to create and verify a fixed-supply SPL token on Solana.",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "create": "node src/create_fixed_supply_token.js",
    "verify": "node src/verify_token.js",
    "calc:test": "node src/test_supply_calc.js",
    "timelock:init": "node src/timelock/initialize_vault.js",
    "timelock:release": "node src/timelock/release_vault.js",
    "allocate:dev+lock": "node src/timelock/allocate_and_lock.js",
    "metadata:update": "node src/metadata/update_metadata.js",
    "governance:preflight": "node src/governance/preflight.js",
    "governance:init": "node src/governance/init_governance.js",
    "governance:delay:toggle": "node src/governance/switch_delay.js toggle",
    "governance:delay:fast": "node src/governance/switch_delay.js 0",
    "governance:delay:prod": "node src/governance/switch_delay.js 86400",
    "program:patch-id": "node src/tools/patch_program_id.js"
  },
  "devDependencies": {},
  "bin": {
    "program:patch-id": "src/tools/patch_program_id.js"
  },
  "optionalScripts": {
    "keygen:wallet": "node src/tools/gen_keypair.js .keys/ops.json",
    "keygen:program": "node src/tools/gen_keypair.js .keys/compliance_wrapper-keypair.json",
    "secret:b64": "node src/tools/print_b64.js"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.95.0",
    "dotenv": "^16.4.0",
    "@coral-xyz/anchor": "^0.30.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.3.0"
  }
}
